<core:FragmentDefinition
xmlns:vh="sap.ui.comp.valuehelpdialog"
xmlns:smartfilterBar="sap.ui.comp.smartfilterbar"
xmlns:fb="sap.ui.comp.filterbar"
xmlns="sap.m"
xmlns:core="sap.ui.core">
<vh:ValueHelpDialog
  id="idMaterialVHDia"
  key="material"
  title="Material"
  supportRanges="false"
  supportMultiselect="false"
  ok=".onMaterialVHDiaOkPress"
  cancel=".onMaterialVHDiaCancelPress">
  <vh:filterBar>
  <fb:FilterBar  id="idMaterialVHDiaFB" search="onMaterialVHDiaSearch">
  <fb:filterGroupItems>
  <fb:FilterGroupItem id="idMaterialVHDiaFB-MaterialFGI" 
    groupName="group1"
    visibleInFilterBar="true"
    name="material/material"
    label="Material">
    <fb:control>
    <Input id="idMaterialVHDFB-MaterialFGI-Input"
    placeholder="Search for Material..."/>
    </fb:control>
  </fb:FilterGroupItem>
  </fb:filterGroupItems>
  </fb:FilterBar>
  </vh:filterBar>

</vh:ValueHelpDialog>
</core:FragmentDefinition>